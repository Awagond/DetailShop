<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="313.0" prefWidth="562.0" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.detailshop.SecondaryController">
    <children>
      <TableView editable="true" prefHeight="200.0" prefWidth="200.0">
        <columns>
          <TableColumn prefWidth="175.0" text="Логин" />
            <TableColumn prefWidth="175.0" text="Роль" />
            <TableColumn prefWidth="175.0" text="Имя" />
        </columns>
      </TableView>
      <Button fx:id="delete" mnemonicParsing="false" text="Удалить" />
        <Button fx:id="Accept" onAction="#switchToPrimary" text="Принято" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
